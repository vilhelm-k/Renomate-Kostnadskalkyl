{
    "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "lib": ["esnext"],
      "strict": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "esModuleInterop": true,
      "sourceMap": true,
      "experimentalDecorators": true,
      "skipLibCheck": true,
      "outDir": "dist",
    }
}